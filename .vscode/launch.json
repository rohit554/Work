            "program": "${file}",
            "cwd": "${workspaceRoot}",
            "env": {
                "PYTHONPATH": "${env:PYTHONPATH};${workspaceRoot}",
                "datagamz_env": "local",
                "datagamz_logging": "DEBUG",
                "SPARK_HOME": "${env:SPARK_HOME}",
                "PYSPARK_PYTHON": "python",
                "PYSPARK_DRIVER_PYTHON": "python",
                "AZURE_CLIENT_ID": "${env:AZURE_CLIENT_ID}",
                "AZURE_USERNAME": "${env:AZURE_USERNAME}",
                "AZURE_PASSWORD": "${env:AZURE_PASSWORD}"
            },
            "console": "integratedTerminal",
             "args": [
                 "--tenant=salmatcolesonline",
                "--run_id=t2",
                 "--extract_start_time=2020-12-01T00:00:00Z",
                 "--extract_end_time=2020-12-02T02:00:00Z",
                 "--api_name=evaluations",
                 "--transformation=fact_conversation_aggregate_metrics",
                 "--org_id=salmatcolesonline",
                 "--pipeline=tp_kpi_raw_data",
                 "--export_name=evaluation_forms_questions",
                 "--input_file=Probe_ColesOnline_20201213.xlsx"
             ]
            /*
            "args": [
                "--env=local",
                "--database_name=holden-prod",
                "--stage_name=QA_SLA_ETL",
                "--step_name=FetchDelta"
            ]
            */
        },
        {
            "name": "Attach to Python Functions",
            "type": "python",
            "request": "attach",
            "port": 9091,
            "preLaunchTask": "func: host start"
        }
    ],
