{
    "configurations": [
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceRoot}",
            "env": {
                "PYTHONPATH": "${env:PYTHONPATH};${workspaceRoot}",
                "datagamz_env": "local",
                "datagamz_logging": "DEBUG",
                "SPARK_HOME": "${env:SPARK_HOME}",
                "PYSPARK_PYTHON": "python",
                "PYSPARK_DRIVER_PYTHON": "python",
                "AZURE_CLIENT_ID": "${env:AZURE_CLIENT_ID}",
                "AZURE_USERNAME": "${env:AZURE_USERNAME}",
                "AZURE_PASSWORD": "${env:AZURE_PASSWORD}"
            },
            "console": "integratedTerminal",
            // "args": [
            //     "--tenant=test",
            //     "--run_id=t2",
            //     "--extract_date=2020-09-02",
            //     "--api_name=wrapup_codes",
            //     "--org_id=SALMATDEV"
            // ]
            "args": [
                "--env=local",
                "--database_name=vodafone-qatar-prod",
                "--stage_name=Raw_FCR",
                "--step_name=FetchDelta"
            ]

        },
        {
            "name": "Attach to Python Functions",
            "type": "python",
            "request": "attach",
            "port": 9091,
            "preLaunchTask": "func: host start"
        }
    ],
    "version": "0.2.0"
}